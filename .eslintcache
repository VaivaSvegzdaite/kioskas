oductInCart(res.data.length);\n  };\n\n  if (userCart) {\n    return (\n      <div className=\"container\">\n        <p className=\"text-left mt-3\">Your shopping cart</p>\n        {userCart.data.length > 0 ? (\n          userCart.data.map((product) => {\n            return (\n              <div key={product.id} className=\"container my-3\">\n                <div className=\"row\">\n                  <table className=\"table\">\n                    <tbody>\n                      <tr>\n                        <th scope=\"row\">{product.id}</th>\n                        <td>\n                          <img\n                            src={img}\n                            className=\"card-img-top\"\n                            style={{ width: 50, height: 50 }}\n                            alt={product.title}\n                          />\n                        </td>\n                        <td>{product.title}</td>\n                        <td>\n                          <button\n                            className=\"btn btn-danger\"\n                            onClick={() => deleteItemFromCart(product.id)}\n                          >\n                            Remove from cart\n                          </button>\n                        </td>\n                      </tr>\n                    </tbody>\n                  </table>\n                </div>\n              </div>\n            );\n          })\n        ) : (\n          <p>is empty</p>\n        )}\n        <Link to={'/'}>\n          <button className=\"btn btn-outline-primary float-right\">\n            Continue shopping\n          </button>\n        </Link>\n      </div>\n    );\n  } else if (!user) {\n    return <p className=\"m-5\">Please login ... </p>;\n  } else {\n    return <p className=\"m-5\">Loading...</p>;\n  }\n};\n\nexport default UserCart;\n","/Users/vaivasvegzdaite/Desktop/react-err-fix/kioskas/src/containers/ProductAdministrationContainer.jsx",[],"/Users/vaivasvegzdaite/Desktop/react